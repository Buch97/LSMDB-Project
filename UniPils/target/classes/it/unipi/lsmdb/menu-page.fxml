<AnchorPane prefHeight="345.0" prefWidth="243.0" style="-fx-background-color: #afb2e1; -fx-background-radius: 20, 20; -fx-padding: 10 15 15 10;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unipi.lsmdb.controller.MenuController">
