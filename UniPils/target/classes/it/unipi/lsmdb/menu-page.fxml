<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="345.0" prefWidth="243.0" style="-fx-background-color: burlywood,peachpuff; -fx-background-radius: 20, 20; -fx-padding: 10 15 15 10;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unipi.lsmdb.controller.MenuController">
   <children>
      <Hyperlink fx:id="usernameLabel" layoutX="50.0" layoutY="27.0" onAction="#onClickProfile" prefHeight="32.0" prefWidth="144.0" text="Username">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Hyperlink>
      <Button fx:id="loginButton" layoutX="65.0" layoutY="75.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="88.0" text="Login">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button onAction="#showCart" layoutX="65.0" layoutY="158.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="88.0" text="Cart">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="65.0" layoutY="117.0" mnemonicParsing="false" onAction="#onClickHomePage" prefHeight="32.0" prefWidth="100.0" text="Homepage">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="65.0" layoutY="197.0" mnemonicParsing="false" onAction="#showWishlist" prefHeight="32.0" prefWidth="88.0" text="Wishlist">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="loginButton1" layoutX="131.0" onAction="#OnlickDebug" layoutY="31.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="88.0" text="Debug">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
