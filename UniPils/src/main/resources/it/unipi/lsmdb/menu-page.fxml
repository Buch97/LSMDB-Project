<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="345.0" prefWidth="243.0" style="-fx-background-color: burlywood,peachpuff; -fx-background-radius: 20, 20; -fx-padding: 10 15 15 10;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unipi.lsmdb.controller.MenuController">
   <children>
      <Hyperlink fx:id="usernameLabel" alignment="CENTER" layoutX="12.0" onAction="#onClickProfile" prefHeight="40.0" prefWidth="219.0" text="Username" underline="true">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Hyperlink>
      <Button fx:id="loginButton" layoutX="78.0" layoutY="73.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="88.0" text="Login">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="buttonCart" layoutX="78.0" layoutY="201.0" mnemonicParsing="false" onAction="#showCart" prefHeight="32.0" prefWidth="88.0" text="Cart">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="72.0" layoutY="141.0" mnemonicParsing="false" onAction="#onClickHomePage" prefHeight="32.0" prefWidth="100.0" text="Homepage">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="buttonWish" layoutX="78.0" layoutY="264.0" mnemonicParsing="false" onAction="#showWishlist" prefHeight="32.0" prefWidth="88.0" text="Wishlist">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
